{
    "private": true,
    "scripts": {
        "bootstrap": "lerna bootstrap",
        "dev": "dumi dev",
        "start": "dumi dev",
        "serve": "serve -s dist",
        "build": "dumi build",
        "build-lib": "father-build",
        "changelog": "lerna-changelog",
        "clean": "lerna clean -y",
        "test": "umi-test --coverage",
        "debug": "umi-test",
        "publish": "npm run build-lib && lerna publish",
        "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx  --fix --format=pretty ./packages && npm run lint:prettier",
        "lint:prettier": "npm run prettier &&  prettier --version && prettier --check \"packages/**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
        "prettier": "prettier --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
        "update:deps": "yarn upgrade-interactive --latest",
        "ycom": "cd packages/components && yalc publish",
        "yalc-pub": "yarn build-lib && yarn ycom"
    },
    "devDependencies": {
        "@ant-design/icons": "^4.7.0",
        "@tailwindcss/postcss7-compat": "^2.2.17",
        "@types/jest": "^24.0.13",
        "@types/react": "^17.0.34",
        "@types/react-dom": "^17.0.11",
        "@types/react-syntax-highlighter": "^13.5.2",
        "@types/tailwindcss": "^2.2.4",
        "@umijs/fabric": "^2.8.1",
        "@umijs/plugin-esbuild": "^1.4.1",
        "ahooks": "next",
        "antd": "^4.16.13",
        "autoprefixer": "^10.4.0",
        "babel-plugin-import": "^1.13.3",
        "babel-plugin-version": "^0.2.3",
        "classnames": "^2.3.1",
        "clsx": "^1.1.1",
        "dumi": "^1.1.32",
        "father-build": "^1.20.5-0",
        "jsx-to-string": "^1.4.0",
        "lerna": "^4.0.0",
        "lerna-changelog": "^2.2.0",
        "postcss": "^8.3.11",
        "postcss-preset-env": "^6.7.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-element-to-jsx-string": "^14.3.4",
        "react-syntax-highlighter": "^15.4.4",
        "tailwind-override": "^0.5.0",
        "tailwindcss": "^2.2.17",
        "tailwindcss-pseudo-elements": "^2.0.0",
        "umi-plugin-tailwindcss": "^3.2.1",
        "umi-test": "^1.9.6",
        "umi-tools": "^0.4.0",
        "umi-types": "^0.5.14"
